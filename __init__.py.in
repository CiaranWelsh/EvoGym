from evogen.evogen import BasicRNG, UniqueReactionsRNG, RNGFactory, eRNG


__version__ = "@EVOGEN_VERSION@"


def runTests():
    import os
    current_directory = os.path.dirname(__file__)
    test_file = os.path.join(current_directory, "evogen_tests.py")
    if not os.path.isfile(test_file):
        raise FileNotFoundError(test_file)
    os.system(f"python {test_file}")


